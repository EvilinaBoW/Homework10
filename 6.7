using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp5
{
    internal class Program
    {
        static void Main(string[] args)
        {
            // 19
            PrintValues(1.1, 2.9, 0.1, x => Math.Pow(x, 3) - 6 * x * x + 2);

            //20
            PrintValues(1, 3, 1, x => 3 * Math.Sin(Math.Sqrt(x)) + 0.25 * x - 3);

            // 21
            PrintValues(0, 2, 1, x => 3 * Math.Cos(x) - Math.Abs(x - 4) + 2);

            // 22
            PrintValues(1.1, 2.9, 0.1, x => 0.25 * x * x - 2.8 * x - 2);
      
            //23
            PrintValues(1, 3, 1, x => Math.Log(x * x) - 1.8 * Math.Sin(x));
      
            // 24
            PrintValues(0, 3, 1, x => x * x + 4 * Math.Sin(Math.PI * x));
       
            // 25
            PrintValues(1, 2.9, 0.9, x => 0.5 * x * x - 1 - Math.Log(x - 3));

            // 26
            PrintValues(0, 3, 1, x => Math.Sqrt(1 + x) - 3 * Math.Cos(x));
    
            // 27
            PrintValues(1, 3, 1, x => Math.Log(x * x) + x - 5);
    
            // 28
            PrintValues(0, 2, 1, x => Math.Pow(x, 3) - 1.75 * x + 0.75);
  
            // 29
            PrintValues(0, 2, 1, x => 0.5 * x - 1 - 2 * Math.Cos(x + Math.PI / 4));
 
            // Вариант 30
            PrintValues(1.1, 3.1, 1, x => 3 * x - 2 * Math.Log(x) - 5);
        }

        static void PrintValues(double a, double b, double h, Func<double, double> f)
        {
            for (double x = a; x <= b; x += h)
                Console.WriteLine($"x = {x:F2}, y = {f(x):F4}");
        }
    }
}
